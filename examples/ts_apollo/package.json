{
  "name": "ts_apollo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "dev": "concurrently --names 'CodeGen,Nodemon' -c 'bgYello,bgBlue' \"graphql-codegen --watch 'src/**/*'\" \"nodemon\"",
    "start": "ts-node src/index.ts",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "graphql": "^16.8.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@types/node": "^20.8.10",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.3.0"
  }
}
